{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "alphagov/notifications-php-client": "^4.1",
        "aws/aws-sdk-php-symfony": "~2.0",
        "beberlei/doctrineextensions": "^1.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.4.0",
        "doctrine/doctrine-migrations-bundle": "2.2.3",
        "doctrine/inflector": "^2.0.0",
        "doctrine/orm": "^2.9.0",
        "lukemadhanga/php-document-parser": "*",
        "php-http/guzzle6-adapter": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^5.3",
        "shuchkin/simplexlsx": "^1.0",
        "squizlabs/php_codesniffer": "3.*",
        "symfony/browser-kit": "^4.4",
        "symfony/console": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/debug-bundle": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/error-handler": "4.4.*",
        "symfony/expression-language": "^4.4",
        "symfony/flex": "^1.17.2",
        "symfony/form": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/mime": "^6.0",
        "symfony/monolog-bundle": "^3.8",
        "symfony/property-access": "^4.4",
        "symfony/property-info": "^4.4",
        "symfony/security-bundle": "^4.4",
        "symfony/security-csrf": "^4.4",
        "symfony/serializer": "^4.4",
        "symfony/stopwatch": "^4.4",
        "symfony/twig-bundle": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/web-profiler-bundle": "^4.4",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "behat/common-contexts": "1.2.*",
        "behat/mink": "^1.8",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-goutte-driver": "^1.2",
        "acsauk/mink-extension": "^2.5.2",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "egulias/email-validator": "^1.2.8|~2.0",
        "friends-of-behat/symfony-extension": "^2.0",
        "mockery/mockery": "^1.2",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan-symfony": "^0.12.28",
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^4.4"
    },
    "repositories": [
        {
            "url": "git@github.com:acsauk/MinkExtension.git",
            "type": "vcs"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.4"
        }
    }
}
