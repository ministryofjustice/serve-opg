{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "alphagov/notifications-php-client": "^5.0",
        "aws/aws-sdk-php-symfony": "~2.0",
        "beberlei/doctrineextensions": "^1.1",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.4.0",
        "doctrine/doctrine-migrations-bundle": "3.3",
        "doctrine/inflector": "^2.0.0",
        "doctrine/orm": "^2.9.0",
        "lukemadhanga/php-document-parser": "^0.1.0",
        "guzzlehttp/guzzle": "^7.4",
        "php-http/guzzle7-adapter": "^1.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "shuchkin/simplexlsx": "^1.0",
        "squizlabs/php_codesniffer": "3.*",
        "symfony/browser-kit": "^5.4",
        "symfony/console": "^5.4",
        "symfony/css-selector": "^5.4",
        "symfony/debug-bundle": "^5.4",
        "symfony/doctrine-bridge": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/error-handler": "5.4.*",
        "symfony/expression-language": "^5.4",
        "symfony/flex": "^2.4.3",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/intl": "^5.4.0",
        "symfony/mime": "^4.3",
        "symfony/monolog-bridge": "^5.4.0",
        "symfony/monolog-bundle": "^3.8",
        "symfony/property-access": "^5.4",
        "symfony/property-info": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/security-csrf": "^5.4",
        "symfony/serializer": "^5.4",
        "symfony/stopwatch": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/web-profiler-bundle": "^5.4",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
        "acsauk/mink-extension": "^2.5.2",
        "behat/behat": "^3.8",
        "behat/common-contexts": "1.2.*",
        "behat/mink": "^1.8",
        "doctrine/doctrine-fixtures-bundle": "^3.5.1",
        "egulias/email-validator": "^3.2.6",
        "friends-of-behat/symfony-extension": "^2.0",
        "mockery/mockery": "^1.2",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^1.10.0",
        "phpstan/phpstan-symfony": "^1.3.0",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.18.13",
        "symfony/phpunit-bridge": "^5.4",
        "symplify/easy-coding-standard": "^12.1"
    },
    "repositories": [
        {
            "url": "git@github.com:acsauk/MinkExtension.git",
            "type": "vcs"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.4"
        }
    }
}
