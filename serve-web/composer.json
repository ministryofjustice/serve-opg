{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.4.8",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "alphagov/notifications-php-client": "^5.0",
        "aws/aws-sdk-php-symfony": "^2.9",
        "beberlei/doctrineextensions": "^1.5",
        "doctrine/doctrine-bundle": "^2.18.2",
        "doctrine/doctrine-migrations-bundle": "^3.7.0",
        "doctrine/inflector": "^2.1",
        "doctrine/orm": "^3.6.1",
        "eightpoints/guzzle-bundle": ">=8.6",
        "lukemadhanga/php-document-parser": "^0.1.5",
        "guzzlehttp/guzzle": "^7.10",
        "php-http/guzzle7-adapter": "^1.1",
        "phpdocumentor/reflection-docblock": "^5.6.6",
        "shuchkin/simplexlsx": "1.1.16",
        "squizlabs/php_codesniffer": "^3.13.5",
        "symfony/browser-kit": "^6.4.31",
        "symfony/console": "^6.4.31",
        "symfony/css-selector": "^6.4.24",
        "symfony/debug-bundle": "^6.4.27",
        "symfony/doctrine-bridge": "^6.4.26",
        "symfony/dotenv": "^6.4.30",
        "symfony/error-handler": "^6.4.26",
        "symfony/expression-language": "^6.4.30",
        "symfony/flex": "^2.10",
        "symfony/form": "^6.4.31",
        "symfony/framework-bundle": "^6.4.31",
        "symfony/intl": "^6.4.30",
        "symfony/mime": "^6.4.30",
        "symfony/monolog-bridge": "^6.4.28",
        "symfony/monolog-bundle": "^3.11.1",
        "symfony/property-access": "^6.4.31",
        "symfony/property-info": "^6.4.31",
        "symfony/security-bundle": "^6.4.30",
        "symfony/security-csrf": "^6.4.31",
        "symfony/serializer": "^6.4.31",
        "symfony/stopwatch": "^6.4.24",
        "symfony/twig-bundle": "^6.4.31",
        "symfony/validator": "^6.4.31",
        "symfony/web-profiler-bundle": "^6.4.27",
        "symfony/yaml": "^6.4.30"
    },
    "require-dev": {
        "behat/behat": "^3.29",
        "behat/common-contexts": "1.2.*",
        "behat/mink": "^1.13",
        "behat/mink-browserkit-driver": "^2.3",
        "behat/mink-goutte-driver": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.7.3",
        "friends-of-behat/mink-extension": "^2.7.5",
        "friends-of-behat/symfony-extension": "^2.6.2",
        "mockery/mockery": "^1.6.12",
        "phpspec/prophecy-phpunit": "^2.4",
        "phpstan/phpstan": "^1.12.32",
        "phpstan/phpstan-symfony": "^1.4.16",
        "phpunit/phpunit": "^9.6.31",
        "symfony/phpunit-bridge": "^6.4.26",
        "symplify/easy-coding-standard": "^12.6.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.4"
        }
    }
}
